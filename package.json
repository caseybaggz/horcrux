{
  "name": "react-horcrux",
  "version": "1.0.5",
  "description": "React web UI Library",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src/ --out-dir dist/",
    "precommit": "lint-staged",
    "prepare": "yarn run build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "test": "NODE_PATH=./src jest"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/caseybaggz/horcrux"
  },
  "keywords": [
    "react",
    "react",
    "component",
    "react",
    "ui",
    "library",
    "ui",
    "library",
    "jest",
    "es6",
    "babel",
    "react",
    "web",
    "enzyme",
    "styled-components"
  ],
  "author": {
    "name": "Casey Baggz",
    "email": "caseybaggz@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caseybaggz/horcrux/issues"
  },
  "homepage": "https://github.com/caseybaggz/horcrux",
  "dependencies": {
    "animated": "^0.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.3.3",
    "@storybook/storybook-deployer": "^2.2.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.63.1",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "jest-enzyme": "^4.0.2",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2"
  },
  "lint-staged": {
    "src/**/*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ]
  }
}
